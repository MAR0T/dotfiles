alias unmountVeraCrypt="sudo veracrypt --text --dismount && sudo rm -rf /run/media/$USER/VeraCrypt/"
alias ls="exa -lah"
alias news="newsboat"
alias book="foliate"
alias unity="$HOME/AppImages/UnityHub.AppImage --no-sandbox"
alias vi="vim"
alias screenshot="xfce4-screenshooter"
alias dotfiles="git --git-dir=$HOME/.dotfiles.git --work-tree=$HOME"
alias wiki="obsidian &"
alias monitor1mode="xrandr --output HDMI-5 --off --output DP-1-1 --off"
alias monitor3mode="xrandr --output HDMI-5 --mode 1920x1080 --refresh 60 --output DP-3 --primary --mode 1920x1080 --rate 164.83 --right-of HDMI-5 --output DP-1-1 --mode 1920x1080 --rate 74.99 --right-of DP-3"
